{
  "dictionaries": ["custom-words"],
  "dictionaryDefinitions": [
    {
      "name": "custom-words",
      "path": "./cspell-wordlist.txt",
      "addWords": true
    }
  ],
  "ignoreRegExpList": [
    "/(```+)[\\s\\S]+?\\1/g",
    "`([^`]*)`",
    "/:[a-zA-Z0-9-_\\+]+:/g"
  ],
  "ignorePaths": [
    "docs/cli",
    "docs/native",
    "versioned_docs/**/api",
    "versioned_docs/**/cli",
    "versioned_docs/**/native",
    "node_modules"
  ],
  "flagWords": [
    "hte"
  ]
}
