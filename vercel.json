      "source": "/docs/angular",
      "destination": "/docs/angular/overview"
    },
    {
      "source": "/docs/:version(v6|v7)/angular",
      "destination": "/docs/:version/angular/overview"
    },
    {
      "source": "/docs/angular/your-first-app/2-taking-photos",
      "destination": "/docs/angular/your-first-app/taking-photos"
    },
    {
      "source": "/docs/angular/your-first-app/3-saving-photos",
      "destination": "/docs/angular/your-first-app/saving-photos"
